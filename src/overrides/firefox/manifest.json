{
  "manifest_version": 2,
  "name": "TabEater",
  "version": "1.1.0",
  "description": "Clear your tab list quickly and keep your browser tidy.",
  "author": "Your Name",
  "homepage_url": "https://github.com/<your-username>/tabeater",
  "permissions": ["tabs", "storage"],
  "background": { "scripts": ["background.js", "background.suggestions.js"] },
  "browser_action": {
    "default_title": "Feed TabEater",
    "default_popup": "popup/popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "applications": {
    "gecko": { "id": "tabeater@yourname.dev", "strict_min_version": "78.0" }
  },
  "options_ui": { "page": "options/options.html", "open_in_tab": true }
}
